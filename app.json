{
  "expo": {
    "name": "expense-tracker-app",
    "slug": "expense-tracker-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splashImage.png",
        "backgroundColor": "#000000"
      },
      "package": "com.yashmistry2144.expensetrackerapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
      
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6bda519d-cf33-40ba-93b3-ee29cb289a6f"
      }
    }
  }
}
